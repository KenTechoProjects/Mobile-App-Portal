@{
    var imgPath = ViewBag.Imagepath;

    if (!string.IsNullOrEmpty(imgPath))
    {
        <img src="@imgPath" alt="No document" id="imageUrl" height="500" width="500" />
    }
    else
    {
        <img src="" alt="No document" id="imageUrl" height="500" width="500" />
    }
}



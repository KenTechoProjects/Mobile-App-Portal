
@model IEnumerable<YellowStone.Web.ViewModels.OnboardingHistoryViewModel>
    <div class="row">
        <div class="col-lg-4"></div>
        <div class="col-lg-4 text-center">
            <p style="font-weight:800">Onboarding History</p>
        </div>
        <div class="col-lg-4"></div>
    </div>

    @if (Model.Any())
    {
        <div class="row">
            <div class="col-lg-1"></div>
            <div class="col-lg-10" style="padding:20px 40px 20px 40px; border: 1px ridge black; border-radius:20px">
                <table class="table table-striped table-condensed">
                    <thead>
                        <tr>
                            <th>
                                Date
                            </th>
                            <th>
                                Activity
                            </th>
                            <th>
                                Time
                            </th>
                            <th>
                                Branch
                            </th>
                            <th>
                                Initiated By
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Date)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Activity)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Time)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AccountBranchCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.InitiatedBy)
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <div class="col-lg-2"></div>

        </div>

      

    }
    else
    {
        <div class="row">
            <div class="col-lg-12 text-center">

                <label style="font-weight:500;">No Onboarding History</label>

            </div>
        </div>

    }
